{
  "manifest_version": 3,
  "name": "UPA Copier",
  "version": "1.04",
  "description": "Copies PO table and Cart Table",
  "update_url": "https://raw.githubusercontent.com/naguibamged/PO-Copier-Ext/main/updates.xml",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "UPA Copier by Naguib Amged",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["panel.html", "panel.css"],
      "matches": ["https://sc.upa.gov.eg/*"]
    }
  ]
}
